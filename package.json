{
  "name": "extending-gutenberg",
  "version": "1.0.0",
  "description": "A plugin to demonstrate extension points in Gutenberg",
  "scripts": {
    "build": "webpack --mode=production",
    "dev": "webpack --watch --mode=development",
    "setup": "npm install && npm run build && npm run build:dashboard",
    "start": "wp-scripts start",
    "build:dashboard": "wp-scripts build ./src/dashboard/index.js --output-path=build --output-filename=dashboard.js",
    "start:dashboard": "wp-scripts start ./src/dashboard/index.js --output-path=build --output-filename=dashboard.js"
  },
  "author": "Ryan Welcher",
  "license": "GPL 2.0",
  "devDependencies": {
    "@babel/core": "^7.9.6",
    "@babel/preset-env": "^7.9.6",
    "@babel/preset-react": "^7.9.4",
    "@wordpress/dependency-extraction-webpack-plugin": "^2.6.0",
    "@wordpress/eslint-plugin": "^5.1.0",
    "@wordpress/scripts": "^9.0.0",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.1.0",
    "clean-webpack-plugin": "^3.0.0",
    "eslint": "^6.8.0",
    "eslint-loader": "^4.0.2",
    "html-loader": "^1.1.0",
    "markdown-loader": "^5.1.0",
    "marked": "^1.0.0",
    "react-markdown": "^4.3.1",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11",
    "webpackbar": "^4.0.0"
  },
  "dependencies": {
    "react-google-charts": "^3.0.15",
    "react-syntax-highlighter": "^12.2.1",
    "uuidv4": "^6.0.8"
  }
}
